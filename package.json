{
  "name": "sgg-api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.2.1",
    "npm": "^5.3.0",
    "yarn": "^0.27.5"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "scripts": {
    "lint": "eslint src test tools migrations seeds",
    "lint:fix": "yarn lint -- --fix",
    "check": "flow check",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "start": "node tools/run.js",
    "build": "node tools/build.js",
    "build:watch": "node tools/build.js --watch",
    "db:version": "node tools/db.js version",
    "db:migrate": "node tools/db.js migrate",
    "db:rollback": "node tools/db.js rollback",
    "db:migration": "node tools/db.js migration",
    "db:seed": "node tools/db.js seed",
    "docker:install": "docker-compose run --rm --no-deps api yarn install",
    "docker:upgrade": "docker-compose run --rm --no-deps api yarn upgrade",
    "docker:test": "docker-compose run api yarn test",
    "docker:test:watch": "docker-compose run api yarn test:watch",
    "docker:db:version": "docker-compose exec api yarn db:version",
    "docker:db:migrate": "docker-compose exec api yarn db:migrate",
    "docker:db:rollback": "docker-compose exec api yarn db:rollback",
    "docker:db:seed": "docker-compose exec api yarn db:seed",
    "heroku-postbuild": "yarn build && yarn db:migrate"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dedent": "^0.7.0",
    "express": "^4.15.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.4",
    "faker": "^4.1.0",
    "graphql": "^0.10.5",
    "graphql-redis-subscriptions": "^1.2.0",
    "graphql-server-express": "^1.0.4",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tools": "^1.1.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "mailchimp-api-v3": "^1.7.0",
    "node-fetch": "^1.7.1",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.0.2",
    "pretty-error": "^2.1.1",
    "redis": "^2.7.1",
    "source-map-support": "^0.4.15",
    "subscriptions-transport-ws": "^0.8.1",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-inline-import-graphql-ast": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "chokidar": "^1.7.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.6.1",
    "flow-bin": "^0.52.0",
    "graphql-tag": "^2.4.2",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.4",
    "webpack-node-externals": "^1.6.0"
  }
}
